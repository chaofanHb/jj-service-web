{{#each items}}
    <tr cCode="{{fromCompany}}">
        <td><input type="checkbox" class="checkinput"/></td>
        <td>{{companyName}}</td>
        <td>{{companyUser}}</td>
        <td>{{companyMobile}}</td>
        <td>{{formtSType type}}</td>
        <td>{{formatDate fromDatetime}}</td>
    </tr>
{{/each}}