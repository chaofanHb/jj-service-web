{{#each items}}
    <tr code="{{code}}">
        <td><input type="checkbox" class="checkinput"/></td>
        <td>{{name}}</td>
        <td>{{formtPKind expPosition}}</td>
        <td>{{expProvoice}}{{expCity}}</td>
        <td>{{formatSStatus status}}</td>
        <td>{{formatDate publishDatetime}}</td>
    </tr>
{{/each}}